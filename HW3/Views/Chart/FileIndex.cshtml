@model List<HW3.Models.SavedReport>

<table>
    @foreach (var file in Model)
    {
        <tr>
            <td>@file.FileName</td>
            <td><a href="@Url.Action("Download", new { fileName = file.FileName })">Download</a></td>
            <td><a href="@Url.Action("Delete", new { fileName = file.FileName })">Delete</a></td>
        </tr>
    }
</table>
@using (Html.BeginForm("Upload", "File", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" />
    <input type="submit" value="Upload" />
}